<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.redclip.faq.model.dao.FaqMapper">

	<select id="selectAll" resultType="faq">
		SELECT
		  FAQ_NO faqNo,
	      QUESTION question,
	      ANSWER answer,
	      FAQ_TYPE faqType
	  FROM
	      TB_FAQ
	</select>
	
	
</mapper>